			<Code Language="csharp">
        <![CDATA[Stopwatch Clock = new Stopwatch();
            Clock.Restart();
            
            $selected$$end$
            
            Console.WriteLine("$classname$.$method$() Excuted in: "+Clock.ElapsedMilliseconds.ToString());
            global::System.Windows.Forms.MessageBox.Show("$classname$.$method$() Excuted in: "+Clock.ElapsedMilliseconds.ToString());
            ]]>
